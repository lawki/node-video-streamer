doctype
html
  head
    title=title
  body
    h3 Folders
    ul
       - var i = 0
       while i < dirs.length
         li
          a(href=dirsEncoded[i]) #{dirs[i]}
          - i++
    h3 Files
    ul
        - var j = 0
        while j < files.length
          li
           a(href='get/'+filesEncoded[j]) #{files[j]}
           - j++
